@CHARSET "UTF-8";

body {
	margin: 0;
	font-size: 0.75em;
	color: #333;
}

select[disabled],
input[disabled],
textarea[disabled],
.btn[disabled],
.disabled {
  opacity: 0.5;
  filter: alpha(opacity=50);
  cursor: not-allowed;
}

.intNum{
	ime-mode:disabled;
}

div.wrap { /* text-align: left; */
	margin: 10px 0 0 10px;
	padding: 0;
	font-family: 맑은 고딕, Trebuchet MS, Verdana, Geneva, sans-serif;
	font-size: 13px;
}
input {
	border: 0px solid #f8f8f8;
	border-radius: 2px;
}
input, select, div.subTab, div.subTabon {
	font-family: 맑은 고딕, Trebuchet MS, Verdana, Geneva, sans-serif;
	font-size: 11px;
}

.clear {
	clear: both;
}

div.tab {
	float: left;
}

div.zTop {
	border-bottom: 1px solid gainsboro;
}

div.blank{
	clear: both;
	height:10px;
}

div.manageTab,div.subTab {
	background: url(../images/cssimg/malis2tab.gif) no-repeat;
	color: #888;
	float: left;
	width: 68px;/* 81px; */
	height: 26px;
	line-height: 29px;
	text-align: center;
	display: block;
	position: relative;
	z-index: 1;
}

div.manageTabon,div.subTabon {
	cursor: pointer;
	background: url(../images/cssimg/malis2tabon.gif) no-repeat;
	color: #535353;
	font-weight:bold;
	float: left;
	width: 68px;/* 81px; */
	height: 26px;
	line-height: 29px;
	text-align: center;
	display: block;
	position: relative;
	z-index: 1;
}

div.manageTab:hover,div.subTab:hover {
	cursor: pointer;
	color: orange !important;
	background: url(../images/cssimg/malis2tabon.gif) no-repeat;
}

.table_style {
	float: left;
}

.table_style ul{
	clear: left;
	margin: 0;
	padding: 0;
	list-style-type: none;
}
.table_main input, .table_main select {
	margin-left: 3px;
	text-align: left !important;
}
.table_main ul li:nth-child(odd){
	width: 90px;
	font-weight: bold;
	text-align: center;
	
	font-size: 12px;
	color:white;
	background-color: #6C677E;
	border-top: 1px solid #7E798C;
	border-bottom: 1px solid #4E4765;
	/* text-align:justify; 
	text-justify:inter-cluster; */
	/* border-top: 1px solid #000; */
}
.table_main ul li:nth-child(even){
	width: 140px;
	
	border-left: 1px solid #FFFFFF;
	border-top: 1px solid #EBE9EE;
	border-bottom: 1px dotted #fff;
	border-right: 1px solid #EBE9EE;
}

.table_main > ul:first-child  > li:first-child {
	border-top-left-radius: 5px;
}
.table_main > ul:first-child  > li:last-child {
	border-top-right-radius: 5px;
}

.table_main > ul:last-child  > li:first-child {
	border-bottom-left-radius: 5px;
}
.table_main > ul:last-child  > li {
	border-bottom: 1px solid #EBE9EE !important;
}
.table_main > ul:last-child  > li:last-child {
	border-bottom-right-radius: 5px;
}

.doubleS {
	width: 376px !important;
}

.table_style .header{
	margin-top: 5px;
	font-weight: bold;
	text-align: center;
	color: white;
	border-collapse:collapse;
}
.table_style .header:first-child li:first-child {
	border-top-left-radius: 5px;
}
.table_style .header:first-child li:last-child {
	border-top-right-radius: 5px;
}
.table_style .header:first-child li{
	border-bottom: 1px solid #4E4765;
}
.table_style .header li{
	font-size: 11px;
	background-color: #6C677E;
	border-left: 1px solid #7E798C;
	border-right: 1px solid #4E4765;
}
.table_style .tbody li:first-child {
    border-left: 1px solid #DFDCE3;
}
.table_style .tbody li:last-child {
    border-right: 1px solid #DFDCE3;
}
.table_style .tbody li{
	font-size: 11px;
	border-top: 1px solid #FFFFFF;
	border-left: 1px solid #EBE9EE;
	border-right: 1px dotted #fff;
	border-bottom: 1px solid #EBE9EE;
}
.table_style .tbody:last-child li{
	border-bottom: 1px solid #DFDCE3 !important;
}
.table_style .tbody:last-child li:first-child{
	border-bottom-left-radius: 5px;
}
.table_style .tbody:last-child li:last-child{
	border-bottom-right-radius: 5px;
}
.table_style .header li, .table_style .tbody li{
	rules:none;
	border-collapse:collapse;
	width: 106px;
	text-align:center;
}

.table_style .header .w4x,.table_style .tbody .w4x{
	width: 436px;
}
.table_style .header .w4x2,.table_style .tbody .w4x2{
	width: 440px;
}

.table_style .header .w3x,.table_style .tbody .w3x{
	width: 326px;
}

.table_style .header .h2x,.table_style .tbody .h2x{
	height: 49px;
}
.table_style .header .h3x,.table_style .tbody .h3x{
	height: 76px;
}
.table_style .header .h4x,.table_style .tbody .h4x{
	height: 103px;
}
.table_style .header .h5x,.table_style .tbody .h5x{
	height: 130px;
}
.table_style .header .h6x,.table_style .tbody .h6x{
	height: 157px;
}
.table_style .header .h7x,.table_style .tbody .h7x{
	height: 184px;
}


.table_style .tbody{
	border-collapse:collapse;
	text-align: center;
}


.table_style .x2{
	width: 215px !important;
}

.table_style ul li{
	float: left;
	height: 22px;
	line-height: 22px;/*  */
	margin: 0;
	padding: 2px 1px;
}

.table_style .tbody li  input {
	height:18px;
}

