@charset "utf-8";

/****************************************************** 레이아웃 박스 스타일 ******************************************************/

#containerWrap { padding:25px; }


/****************************************************** 레이아웃 디테일 스타일 ******************************************************/
/*탭*/
.gtabZone { float:none; *zoom:1; background:url(../images/common/bg_gtab.gif) repeat-x 0 top; height:37px; overflow:hidden; border-left:#bababa solid 1px; }
.gtabZone:after { content:""; display:block; clear:both; height:0; }
.gtabZone ul { margin-top:12px; margin-left:25px; }
.gtabZone li { float:left; background:url(../images/common/bg_gtab_off.gif) repeat-x left bottom; margin-left:3px; line-height:1.6; height:24px; }
.gtabZone li:first-child { margin-left:0px; }
.gtabZone li a { background:url(../images/common/bg_gtab_off_end.gif) no-repeat right bottom; height:16px; line-height:1.6; padding:5px 10px 3px 10px; display:inline-block; color:#81878e;  }
.gtabZone li a em { font-size:11px; margin-left:3px; }
.gtabZone li.on { background:url(../images/common/bg_gtab_on.gif) repeat-x left bottom; }
.gtabZone li.on a { background:url(../images/common/bg_gtab_on_end.gif) no-repeat right bottom; height:16px; line-height:1.6; padding:5px 10px 3px 10px; display:inline-block; color:#202020;  font-weight:bold; }

.gtabZone2 { float:none; *zoom:1; background:url(../images/common/bg_gtab2.gif) repeat-x 0 top; height:37px; overflow:hidden; margin-top:5px; }
.gtabZone2:after { content:""; display:block; clear:both; height:0; }
.gtabZone2 ul { margin-top:12px; }
.gtabZone2 li { float:left; background:url(../images/common/bg_gtab_off.gif) repeat-x left bottom; margin-left:3px; line-height:1.6; height:24px; }
.gtabZone2 li:first-child { margin-left:0px; }
.gtabZone2 li a { background:url(../images/common/bg_gtab_off_end.gif) no-repeat right bottom; height:16px; line-height:1.6; padding:5px 10px 3px 10px; display:inline-block; color:#81878e;  }
.gtabZone2 li a em { font-size:11px; margin-left:3px; }
.gtabZone2 li.on { background:url(../images/common/bg_gtab_on.gif) repeat-x left bottom; }
.gtabZone2 li.on a { background:url(../images/common/bg_gtab_on_end.gif) no-repeat right bottom; height:16px; line-height:1.6; padding:5px 10px 3px 10px; display:inline-block; color:#202020;  font-weight:bold; }


/*선택박스*/
.classicBox { float:none; *zoom:1; background:url(../images/common/bg_bookmark_repeat.gif) repeat-x bottom; border:#bbbfc4 solid 1px; box-sizing:border-box; }
.classicBox:after { content:""; display:block; clear:both; height:0px; }

.classicBox dl { float:none; *zoom:1; padding:6px 15px; border-top:#dadada solid 1px; }
.classicBox dl:first-child { border-top:none; }
.classicBox dl:after { content:""; display:block; clear:both; height:0px; }
.classicBox dl dt { width:10%!important; max-width:60px; float:left; display:block; line-height:1.8; color:#242424; }
.classicBox dl dd { width:90%; float:left; display:block; line-height:1.8; }
.classicBox .classic_list { float:none; *zoom:1; }
.classicBox .classic_list:after { content:""; display:block; clear:both; height:0px; }
.classicBox .classic_list li { float:left; margin:0px 0px 0px 25px; line-height:1.8; vertical-align:middle; color:#838383; }
.classicBox .classic_list li input[type=checkbox] { margin-right:4px;}


/*선택결과*/
.chooseResult { margin-top:-4px; margin-bottom:6px; }
.chooseResult dl {float:none; *zoom:1; }
.chooseResult dl:after { content:""; display:block; clear:both; height:0px; }

.chooseResult dl dt { float:left; width:83px; background:url(../images/common/bg_div_chooseresult.gif) no-repeat right center; display:block; color:#242424; font-weight:bold; margin-right:8px; padding-top:2px;  }
.chooseResult dl dd { width:auto; overflow:hidden; margin-right:2px; padding-right:80px; position:relative; }
.chooseResult dl dd a, .chooseResult dl dd span { color:#555; vertical-align:middle; }
.chooseResult dl dd a em { font-size:10px; color:#999; margin-left:4px; vertical-align:middle; }
.chooseResult .btn_allcancel { position:absolute; background:url(../images/common/btn_allcancel.gif) no-repeat right center; display:inline-block; width:63px; height:21px; margin-left:5px; right:0; }
.chooseResult .btn_allcancel em { display:none; }


/*타이틀영역*/
.tb_titleZone { float:none; *zoom:1; margin-bottom:10px; margin-top:25px; }
.tb_titleZone:after { content:""; display:block; clear:both; height:0px; }

.tb_titleZone .title { float:left; line-height:23px; vertical-align:middle; }
.tb_titleZone .title h3 { float:left; background:url(../images/common/h3_title.gif) no-repeat left center; line-height:22px; font-size:14px; font-weight:bold; color:#2e579d; vertical-align:middle; padding-left:12px; margin-right:20px; }
.tb_titleZone .title a { margin-left:6px;}
.btn_change_network { background:url(../images/common/btn_change_network.gif) no-repeat; width:74px; height:23px; display:inline-block; vertical-align:middle; }
.btn_change_network_on { background:url(../images/common/btn_change_network_on.gif) no-repeat; width:74px; height:23px; display:inline-block; vertical-align:middle; }
.btn_add_network { background:url(../images/common/btn_add_network.gif) no-repeat; width:65px; height:23px; display:inline-block; vertical-align:middle; }

.tb_titleZone .btnBox { float:right; }
.tb_titleZone .btnBox li { float:left; }
.tb_titleZone .btnBox .btn_excel { background:url(../images/common/btn_save_excel.gif) no-repeat; width:30px; height:23px; display:inline-block; vertical-align:middle; }
.tb_titleZone .btnBox .btn_print { background:url(../images/common/btn_save_print.gif) no-repeat; width:31px; height:23px; display:inline-block; vertical-align:middle; }
.tb_titleZone .btnBox .btn_option { background:url(../images/common/btn_save_option.gif) no-repeat; width:31px; height:23px; display:inline-block; vertical-align:middle; }
.tb_titleZone .btn_excel em, .tb_titleZone .btn_print em, .tb_titleZone .btn_option em, .btn_change_network em, .btn_add_network em, .btn_change_network_on em { position:absolute; left:-999999999999999999px; overflow:hidden; width:0px; height:0px;}





/* 리스트스타일 */
.itemList { float:none; *zoom:1; width:100%; }
.itemList:after { content:""; display:block; clear:both; height:0; }
.itemList li { float:left; margin-right:25px; line-height:1.6; vertical-align:middle; }
.itemList li:last-child { margin-right:0px; }
