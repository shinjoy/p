/* Modal popup */
.modalWrap { position:relative; width:50%; margin:10% auto 0px; background:#fff; z-index:3; zoom:1; box-shadow:8px 8px 6px rgba(0, 0, 0, 0.5); border:#20304a solid 1px;}
.modalWrap h1 { background:#20304a; color:#fff; padding:5px 15px; height:26px; font-size:12px; line-height:16px; height:16px; }

.modalWrap .btn_modal_min { position:absolute; right:49px; top:8px; background:url(../images/common/btn_modal_min.gif) no-repeat; width:10px; height:9px; display:inline-block; }
.modalWrap .btn_modal_max { position:absolute; right:32px; top:8px; background:url(../images/common/btn_modal_max.gif) no-repeat; width:11px; height:9px; display:inline-block; }
.modalWrap .btn_modal_close { position:absolute; right:15px; top:8px; background:url(../images/common/btn_modal_close.gif) no-repeat; width:11px; height:9px; display:inline-block; }
.modalWrap .btn_modal_max em, .modalWrap .btn_modal_min em, .modalWrap .btn_modal_close em { position:absolute; left:-999999999999999999px; overflow:hidden; width:0px; height:0px; }

.modalWrap .mo_container { margin:0 15px; padding:20px 0 25px; }
.modalWrap .mo_footer { border-top:#dadada solid 1px; padding-bottom:25px; }

.mo_footer .btnZone { margin-top:20px; text-align:center; }
.mo_footer .btnZone a { margin-left:6px; background:url(../images/common/bg_btn_modal.gif) repeat-x; line-height:1.4; padding:4px 8px; text-align:center; min-width:48px; border:#dadada solid 1px;border-radius:2px;-moz-border-radius:2px;-webkit-border-radius:2px; display:inline-block; color:#555; }
.mo_footer .btnZone a strong { color:#242424; }
.mo_footer .btnZone a:first-child { margin-left:0px; }

.modalWrap .gtabZone { border-left:none; }
.modalWrap .gtabZone ul { margin-top:12px; margin-left:16px; }

.modal .bg { position:absolute; background:#000; width:100%; height:100%; opacity:0.5; z-index:2; filter:progid:DXImageTransform.Microsoft.Alpha(opacity=50); zoom:1; }
.modal .fg {position:relative; width:50%; margin:5em auto; background:#fff; padding:0 1em; *padding:1em; border:8px solid #ddd; z-index:3; zoom:1; border-radius:5px; box-shadow:0 0 6px #000; }

.modal .ie6 {position:absolute; left:0; top:0; width:100%; height:100%; border:0; opacity:0; filter:alpha(opacity=0); z-index:1}
.modalActive {position:absolute; top:0; left:0; width:100%; _height:100%; min-height:100%; z-index:100; height:auto!important; padding-bottom:16px; }
.modalClose {position:absolute; right:-8px; top:-8px; border:0; background:#ddd; padding:0; width:28px; height:28px; font-size:14px; font-weight:bold; cursor:pointer; color:#999; border-radius:5px; }
.modalBlur { position:absolute; top:0; right:0; width:1px; height:1px; overflow:hidden; }

.modalWrap h2.title { font-size:12px; font-weight:bold; color:#242424; line-height:1.6; margin-bottom:8px; }
.modalWrap h2.title_arrow { float:none; *zoom:1; background:url(../images/common/h2_mo_title.gif) no-repeat 0 3px; line-height:1.6; font-size:12px; font-weight:bold; color:#345698; vertical-align:middle; padding-left:10px; margin-bottom:5px; position:relative; }
.modalWrap h2.title_arrow:after { content:""; display:block; clear:both; height:0px; }

.title_count { margin:0 0 0 20px; color:#6d7a9e; font-weight:normal;}
.title_arrow_rightBox {float:right; *zoom:1; }
.title_arrow .between { font-weight:normal; color:#6d7a9e; margin-left:6px;}


