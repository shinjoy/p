@charset "utf-8";

@import url('http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css');

.ui-datepicker-calendar > tbody td.ui-datepicker-week-end:first-child a { color:#f00; } 
.ui-datepicker-calendar > tbody td.ui-datepicker-week-end:last-child a { color:#00f; }

/* 고정타이틀 */
.FixTable {
	width:100%;
	position:fixed;
	z-index:10;
	top:0px;
	left:0px;
}

.ConTable {
	position:overflow-y:scroll;
	position:absolute;
	width:100%;
	height:$(document).height() - $('#TitleTable').height() + "px";
}
/* 고정타이틀 */

/* 레이어 */
#ViewDiv {
	position:absolute;
	margin-top:-300px; margin-left:-300px;
	background:#ffffff;
	text-align:left;
	z-index:160;
}

#BackDiv {
	position:absolute;
	width:100%; height:100%;
	background:#000000;
	z-index:50;
}
/* 레이어 */

.InBox {
	background-color:#FFFFFF;
	border: 0px;
	text-align:right;
}

.OutBox {
	background-color:#E7E7E7;
	border: 0px;
	text-align:right;
}

table {
	border-collapse:collapse;	/*td공백시에도 선 유지*/
}

tr {
	height:30px;
}

body {
	font-family:맑은 고딕;
	font-size:10pt;
	margin:0px;
	padding:0px;
	
	scrollbar-base-color: #C0C0C0;
	scrollbar-base-color: #C0C0C0;
	scrollbar-3dlight-color: #C0C0C0;
	scrollbar-highlight-color: #C0C0C0;
	scrollbar-track-color: #EBEBEB;
	scrollbar-face-color:#c4ccd5;
	scrollbar-arrow-color:#5c87bd;
	scrollbar-shadow-color: #C0C0C0;
	scrollbar-dark-shadow-color: #C0C0C0;
}

textarea {
	ime-mode:active;
	width:95%;
	height:70px;
	margin-top:5px;
	margin-bottom:5px;
}

::-webkit-scrollbar {
    width: 6px;
}
 
::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.3); 
    border-radius: 10px;
}
 
::-webkit-scrollbar-thumb {
    border-radius: 10px;
    -webkit-box-shadow: inset 0 0 6px rgba(0,0,0,0.9); 
}

form {
	display:inline;
}

a:link {
	font-size:9pt;
	text-decoration:none;
	color:darkblue;
}

a:visited {
	text-decoration:none;
	color:darkblue;
}

a:hover {
	/*text-decoration:underline;*/
	color:red;
}

a:active {
	text-decoration:none;
	color:red;
}

/*	프로세스 레이어		*/
#prolayer {
	position:absolute;
	left:0px;
	top:0px;
	z-index:100;
	-ms-filter:alpha(opacity=80);
	filter:alpha(opacity=80);
	opacity:0.8;
	-moz-opacity:0.8;
	background-color:#FFFFFF;
}


