/*	버튼	*/
.button, .button a, .button button, .button input {
/*position : relative;*/
margin : 0;
display : inline-block;
border : 0;
white-space : nowrap;
vertical-align : middle;
overflow : visible;
font-size: 8pt; color: #ffffff;
border-radius:5px;
}

.button {
height : 20px;
padding : 0;
margin-right : 2px;
background-position : left top;
}

.button a, .button button, .button input {
left : 2px;
cursor : pointer;
}

.button a, .button button, .button input {
height : 20px;
padding : 0 8px 0 8px;
line-height : 20px;
background-position : right top;
}

.button *:hover, .button *:active, .button *:focus {
color : red;
}

.button a {
padding : 0 10px 0 10px;
}

.btn_Blue, .btn_Blue a, .btn_Blue button, .btn_Blue input {
background-image: url(/PASS/images/sp/CSS/button_blue.gif);
}

.btn_Orange, .btn_Orange a, .btn_Orange button, .btn_Orange input {
background-image: url(/PASS/images/sp/CSS/button_orange.gif);
}

.btn_Gray, .btn_Gray a, .btn_Gray button, .btn_Gray input {
background-image: url(/PASS/images/sp/CSS/button_gray.gif);
font-weight:normal; color:#000000;
}

.btn_LightGray, .btn_LightGray a, .btn_LightGray button, .btn_LightGray input {
background-image: url(/PASS/images/sp/CSS/button_lightgary.gif);
font-weight:normal; color:#000000;
}

.btn_Violet, .btn_Violet a, .btn_Violet button, .btn_Violet input {
background-image: url(/PASS/images/sp/CSS/button_violet.gif);
}

.btn_Green, .btn_Green a, .btn_Green button, .btn_Green input {
background-image: url(/PASS/images/sp/CSS/button_green.gif);
}

.btn_Yellow, .btn_Yellow a, .btn_Yellow button, .btn_Yellow input {
background-image: url(/PASS/images/sp/CSS/button_yellow.gif);
}

.btn_BlueGreen, .btn_BlueGreen a, .btn_BlueGreen button, .btn_BlueGreen input {
background-image: url(/PASS/images/sp/CSS/button_bluegreen.gif);
}

