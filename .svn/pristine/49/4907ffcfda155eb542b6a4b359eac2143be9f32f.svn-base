/****************** tab ******************/
.tabZone_st01 { float:none; *zoom:1; width:100%; height:36px; background:url(../images/common/board/bg_board_right_end.gif) no-repeat right top;  }
.tabZone_st01:after { content:""; display:block; clear:both; height:0; }

.tabZone_st01 li { float:left; height:36px; border-right:#406998 solid 1px; overflow:hidden; }
.tabZone_st01 li a { font-size:13px; line-height:14px; height:14px; vertical-align:middle; padding:10px 40px 11px; display:inline-block; }
.tabZone_st01 li:first-child { background:url(../images/common/board/bg_board_left_end.gif) no-repeat left top; }
.tabZone_st01 li:first-child a { padding-left:48px; }

.tabZone_st01 li.current { background:#4c75a3; }
.tabZone_st01 li.current a { color:#fff; }
.tabZone_st01 li:first-child.current { background:url(../images/common/board/bg_board_left_end_on.gif) no-repeat left top; }


.tabZone_st02 { float:none; *zoom:1; width:100%; padding:0 15px; box-sizing:border-box; }
.tabZone_st02:after { content:""; display:block; clear:both; height:0; }
.tabZone_st02 li { float:left; margin-left:30px; height:27px; overflow:hidden; }
.tabZone_st02 li:first-child { float:left; margin-left:0px;  }
.tabZone_st02 li a {font-size:13px; line-height:15px; height:15px; vertical-align:middle; padding:0px 1px 12px; display:inline-block; color:#537195; }
.tabZone_st02 li.current {border-bottom:#5982b1 solid 2px; font-weight:bold; color:#4b76a3; }

.tabZone_st01 + .tabZone_st02 { margin-top:15px; }

.tabZone_st03 { float:none; *zoom:1; width:100%; height:35px; padding-top:3px; padding-left:3px; /*background:#547197; border:#3c5e8a solid 1px;*/ background:#7d8fa2; border:#63788e solid 1px; box-sizing:border-box; }
.tabZone_st03:after { content:""; display:block; clear:both; height:0; }
.tabZone_st03 li { float:left; /*background:url(../images/common/board/bg_tab03_divide.gif) no-repeat right 9px;*/ background:url(../images/common/bg_tabdiv3.png) no-repeat right 9px; height:29px; overflow:hidden; }
.tabZone_st03 li a {font-size:13px; line-height:15px; height:15px; vertical-align:middle; padding:5px 30px 9px; display:inline-block; color:#fff; }
.tabZone_st03 li.current { border-bottom:#fff solid 2px; }
.tabZone_st03 li.current a { color:#303030; background:#FFF; font-size:14px; font-weight:bold; padding:8px 30px 6px; }


.tabZone_st04 { float:none; *zoom:1; width:100%; height:38px; background:url(../images/common/board/bg_tab04_repeat.gif) repeat-x; border:#d1d1d1 solid 1px; padding:7px 20px 7px; box-sizing:border-box; }
.tabZone_st04:after { content:""; display:block; clear:both; height:0px; }
.tabZone_st04 li { float:left; background:url(../images/common/board/bg_tab04_divide.gif) no-repeat right center; height:28px; overflow:hidden;  }
.tabZone_st04 li:first-child { padding-left:0px; }
.tabZone_st04 li a {font-size:11px; line-height:14px; height:14px; vertical-align:middle; color:#898989; padding:4px 18px 4px; display:inline-block;  }
.tabZone_st04 li.current { padding:0 20px; }
.tabZone_st04 li:first-child.current { padding:0 20px 0 0px; }
.tabZone_st04 li.current a { color:#fff; background:#2d508a; font-size:12px; border-radius:20px; height:14px; padding:3px 15px 5px; }

.tabZone_st03 + .tabZone_st04 { margin-top:10px; }
.tabZone_st04 + .board_classic { margin-top:10px;}
.tabZone_st03 + .board_classic { margin-top:10px;}
.tabZone_st03 + .tabZone_st06 { margin-top:15px; }

.tabZone_st05 { float:none; *zoom:1; width:100%; background:url(../images/common/board/bg_tab05.gif) repeat-x bottom; height:35px; margin-top:3px; }
.tabZone_st05:after { content:""; display:block; clear:both; height:0; }
.tabZone_st05 li { float:left; height:35px;  }
.tabZone_st05 li a { float:left; font-size:12px; line-height:14px; vertical-align:middle; padding:10px 30px 11px; display:inline-block; color:#a5a5a5; }
.tabZone_st05 li.current { background:url(../images/common/board/bg_tab05_on.gif) repeat-x bottom; }
.tabZone_st05 li.current a { font-weight:bold; font-size:14px; color:#73869a; line-height:16px; vertical-align:middle; padding:8px 30px 11px; display:inline-block; }

.tabZone_st06 { float:none; *zoom:1; width:100%; background:url(../images/common/board/bg_tab06_repeat.gif) repeat-x bottom; border:#b3c0d1 solid 1px; padding:6px 15px 7px 0; box-sizing:border-box; }
.tabZone_st06:after { content:""; display:block; clear:both; height:0px; }
.tabZone_st06 li { float:left; background:url(../images/common/board/bg_tab04_divide.gif) no-repeat right center; overflow:hidden; display:inline-block; margin:2px 0; }
.tabZone_st06 li a {font-size:12px; line-height:13px; height:13px; vertical-align:middle; color:#6b7788; padding:4px 13px 4px; display:inline-block;  }
.tabZone_st06 li.current { padding:0 14px; }
.tabZone_st06 li.current a { color:#fff; background:#2d508a; font-size:12px; border-radius:11px; line-height:13px; height:13px; padding:4px 13px 4px; }

.team_mem_List { margin-top:7px; }
.team_mem_List label { margin-right:15px; }
.team_mem_List label input { margin-right:4px;}

