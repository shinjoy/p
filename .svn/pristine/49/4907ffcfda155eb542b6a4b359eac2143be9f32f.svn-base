.tb_list_basic tbody tr.tr_divide_line td, .tb_list_basic tbody tr.tr_divide_line th { border-top:#b9c1ce solid 1px; }

.tb_list_basic tbody tr:nth-child(5n+1) td, .tb_list_basic tbody tr:nth-child(5n+1) th { border-top:#b9c1ce solid 1px; }



