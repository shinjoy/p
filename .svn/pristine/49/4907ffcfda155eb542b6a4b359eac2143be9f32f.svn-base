.communi_inputBox .option_list label { margin-right:20px; }
.communi_inputBox .file_list { vertical-align:middle; padding:7px 15px 8px; border-bottom:#c7cdd8 dashed 1px; text-overflow: ellipsis; -o-text-overflow: ellipsis; overflow:hidden; white-space: nowrap; word-wrap: normal !important; display: block; }
.communi_inputBox .file_list .btn_file { background:url(../images/work/btn_file.gif) no-repeat; width:65px; height:23px; display:inline-block; vertical-align:middle; margin-right:5px; }
.communi_inputBox .file_list .btn_file em { position:absolute; left:-999999999999px; top:-999999999999px; }
.communi_inputBox .file_list .file_name { color:#84888d; font-size:11px; letter-spacing:-0.5px; margin-left:3px; vertical-align:middle; line-height:23px; display:inline-block; line-height:23px; }

.communi_inputBox .txtArea { padding:12px 15px 15px; position:relative; }
.communi_inputBox .txtArea .txtArea_b { border-left:#9fa8b3 solid 1px; border-top:#9fa8b3 solid 1px; border-bottom:#9fa8b3 solid 1px; border-top-left-radius:5px; border-bottom-left-radius:5px; height:53px; width:100%; padding:5px 70px 5px 5px; box-sizing:border-box; }
.communi_inputBox .btn_send { position:absolute; background:url(../images/work/btn_send.gif) no-repeat; width:62px; height:53px; display:inline-block; right:15px; top:12px; }
.communi_inputBox .btn_send em { position:absolute; left:-999999999999px; top:-999999999999px; }



/* 인맥네트워크 */
.people_list_st li { background:url(../images/work/allday_dot.gif) no-repeat left 6px; padding-left:7px; font-size:11px; line-height:15px; margin:3px 0; }
.people_list_st li:first-child { margin-top:0px; }

.people_organ_wrap { width:100%; float:none; *zoom:1; }
.people_organ_wrap:after { content:""; display:block; clear:both; height:0; }
.people_organ_wrap .left_block { float:left; background:#06F; width:15%; }
.people_organ_wrap .right_block { float:right; background:#CCC; width:85%; }


.btn_file_finder { background:url(../images/eam/btn_img_finder.gif) no-repeat; width:50px; height:23px; display:inline-block; vertical-align:middle; margin-right:5px; }
.btn_file_finder em { position:absolute; left:-999999999999px; top:-999999999999px; }