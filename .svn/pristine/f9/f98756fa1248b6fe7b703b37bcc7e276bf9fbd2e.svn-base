.pageIdx{display:inline-block;width:28px;height:20px;}
.pageBtn{display:inline-block;width:20px;height:20px;}
/* btn Common */
.btn:not(.glass),
.btn:not(.glass) a,
.btn:not(.glass) button,
.btn:not(.glass) input{ position:relative; margin:0; display:-moz-inline-stack; display:inline-block; text-decoration:none !important; border:0; font-size:12px;  font-family:맑은 고딕, Trebuchet MS, Verdana, Geneva, sans-serif; white-space:nowrap; background:url(../images/cssbtn/buttonWhite.gif) no-repeat; overflow:visible; color: #000;}
.btn:not(.glass) { padding:0; vertical-align:middle; margin-right:2px; background-position:left top;}
.btn:not(.glass) a,
.btn:not(.glass) button,
.btn:not(.glass) input{ left:2px; vertical-align:top; cursor:pointer; _cursor:hand;}

/* btn Size = medium | large | xLarge | small */
.btn.m:not(.glass), .btn.m:not(.glass) a, .btn.m:not(.glass) input{ height:23px; line-height:21px;}
.btn.l:not(.glass), .btn.l:not(.glass) a, .btn.l:not(.glass) input{ height:30px; line-height:28px;}
.btn.xl:not(.glass), .btn.xl:not(.glass) a, .btn.xl:not(.glass) input{ height:35px; line-height:33px;}
.btn.s:not(.glass), .btn.s:not(.glass) a, .btn.s:not(.glass) input{ height:19px; line-height:15px;}
.btn.m:not(.glass) { background-position:left 0;}
.btn.l:not(.glass) { background-position:left -30px;}
.btn.xl:not(.glass) { background-position:left -65px;}
.btn.s:not(.glass) { background-position:left -106px;}
.btn.m a,.btn.m input{ padding:0 10px 0 8px; font-size:12px; background-position:right top;}
.btn.l a,.btn.l input{ padding:0 10px 0 8px; font-size:16px; background-position:right -30px;}
.btn.xl a,.btn.xl input{ padding:0 10px 0 8px; font-size:20px; background-position:right -65px;}
.btn.s a,.btn.s input{ padding:0 6px 0 4px; font-size:11px; background-position:right -106px;}

/* btn Hover */
.btn *:hover,
.btn *:active,
.btn *:focus{ color:#f90;}

/* btn Strong */
.btn.strong *{ font-weight:bold !important;}

/* btn Icon Variation */
.btn.icon *{ padding-left:20px;}
.btn.s.icon *{ padding-left:16px;}
.btn.icon span{ width:20px; margin-right:-20px; padding:0 !important;}
.btn.m .check{ background-position:4px -150px;}
.btn.l .check{ background-position:4px -146px;}
.btn.xl .check{ background-position:5px -144px;}
.btn.s .check{ background-position:3px -153px;}
.btn.m .add{ background-position:-16px -150px;}
.btn.l .add{ background-position:-14px -146px;}
.btn.xl .add{ background-position:-14px -144px;}
.btn.s .add{ background-position:-18px -153px;}
.btn.m .delete{ background-position:-34px -150px;}
.btn.l .delete{ background-position:-34px -146px;}
.btn.xl .delete{ background-position:-34px -144px;}
.btn.s .delete{ background-position:-37px -153px;}
.btn.m .calendar{ background-position:-56px -150px;}
.btn.l .calendar{ background-position:-56px -146px;}
.btn.xl .calendar{ background-position:-56px -144px;}
.btn.s .calendar{ background-position:-59px -153px;}
.btn.m .refresh{ background-position:-76px -150px;}
.btn.l .refresh{ background-position:-76px -146px;}
.btn.xl .refresh{ background-position:-76px -144px;}
.btn.s .refresh{ background-position:-79px -153px;}

