@charset "utf-8";


/*게시판스타일*/
.board_basic { width:100%; }
.board_basic caption { visibility:hidden; height:0; width:0; overflow:hidden; } 
.board_basic thead th { padding:8px 10px; border-top:#5271b0 solid 2px; border-bottom:#e1e1e1 solid 1px; color:#363636; font-weight:bold; text-align:center; }
.board_basic thead th:first-child { border-left:none; }
.board_basic thead td { border-left:#e3e3e3 solid 1px; border-bottom:#e3e3e3 solid 1px; padding:3px 10px;  text-align:center; }
.board_basic thead td:first-child { border-left:none; }

.board_basic tbody tr:first-child { background:url(../images/sp/css/bg_table_line.gif) repeat-x top; }
.board_basic tbody tr:nth-child(even) { background:#fafafa; }
.board_basic tbody th { border-left:#e3e3e3 solid 1px; padding:8px 15px 8px 15px; border-bottom:#e3e3e3 solid 1px; background:#f5f5f5; font-weight:normal; color:#363636;  text-align:center; }
.board_basic tbody th:first-child { border-left:none; }
.board_basic tbody td { border-left:#e3e3e3 solid 1x; border-bottom:#e3e3e3 solid 1px; padding:8px 15px 8px 15px;  text-align:center; }
.board_basic tbody td:first-child { border-left:none; }

.board_basic_lf { width:100%; }
.board_basic_lf caption { visibility:hidden; height:0; width:0; overflow:hidden; } 
.board_basic_lf thead th { padding:8px 15px; border-top:#5271b0 solid 2px; border-bottom:#e1e1e1 solid 1px; color:#363636; font-weight:bold; text-align:left; }
.board_basic_lf thead th:first-child { border-left:none; }

.board_basic_lf tbody tr:first-child { background:url(../../images/sp/css/bg_table_line.gif) repeat-x top; }
.board_basic_lf tbody tr:nth-child(even) { background:#fafafa; }
.board_basic_lf tbody th:first-child { border-left:none; }
.board_basic_lf tbody td { border-left:#e3e3e3 solid 1x; border-bottom:#e3e3e3 solid 1px; padding:8px 15px 8px 15px;  text-align:left; line-height:15px; vertical-align:middle; }
.board_basic_lf tbody td:first-child { border-left:none; }

.board_basic_lf .center { text-align:center; } 
.board_basic_lf .coname { color:#363636; }
.board_basic_lf .category1 { color:#e91a21; font-size:10px; font-family:Verdana, Geneva, sans-serif; }
.board_basic_lf .category2 { color:#136fe2; font-size:10px; font-family:Verdana, Geneva, sans-serif; }
.board_basic_lf .colink { background:none; padding-left:0px; } 
.board_basic_lf .colink a { background:url(../images/portfolio/btn_go.gif) no-repeat left 2px; width:47px; height:18px; display:block; text-indent:-9999999px; }
.board_basic_lf .colink a:hover { text-decoration:underline; color:#898989; }

/*게시판 페이지버튼 */
.btnListZone { float:none; *zoom:1; width:100%; margin-bottom:15px; }
.btnListZone  ul { float:right; }
.btnListZone  ul li { float:left; }
.btnListZone:after { content:""; display:block; clear:both; height:0; }

.btnPageList{ text-align:center;line-height:normal; font-family:Verdana, Geneva, sans-serif; font-size:10px; color:#363636; margin-top:20px; }
.btnPageList a,
.btnPageList strong { position:relative; display:inline-block; padding:6px 6px; font-weight:bold; text-decoration:none; line-height:1.2; vertical-align:middle; border:#ccc solid 1px; margin:0 1px; }
.btnPageList strong { color:#366bbb; }
.btnPageList a:hover, .btnPageList a:active, .btnPageList a:focus{ text-decoration:underline; }

.btnPageList .direction {font-weight:normal;white-space:nowrap; border:none; }
.btnPageList .direction a { padding:0px 2px; line-height:1.2; vertical-align:middle;  }
.btnPageList .direction img { vertical-align:middle;  }



/* table */
.tb_basic { width:100%; border-top:#5271b0 solid 2px; border-bottom:#5271b0 solid 1px; }
.tb_basic caption { display:none; height:0; width:0; overflow:hidden; } 
.tb_basic thead th { border-left:#e1e1e1 solid 1px; padding:8px 10px; border-bottom:#e1e1e1 solid 1px; background:#eee; color:#242424; }
.tb_basic thead th:first-child { border-left:none; }
.tb_basic thead td { border-left:#e1e1e1 solid 1px; border-bottom:#e1e1e1 solid 1px; padding:3px 10px; }
.tb_basic thead td:first-child { border-left:none; }

.tb_basic tbody th { border-left:#e1e1e1 solid 1px; padding:8px 10px; border-bottom:#e1e1e1 solid 1px; background:#f5f5f5; font-weight:normal; }
.tb_basic tbody th:first-child { border-left:none; }
.tb_basic tbody td { border-left:#e1e1e1 solid 1px; border-bottom:#e1e1e1 solid 1px; padding:3px 10px; text-align:center; }
.tb_basic tbody td:first-child { border-left:none; }
.tb_basic .left { text-align:left; }


.tb_l_basic { width:100%; border-top:#5271b0 solid 2px; border-bottom:#d4d4d4 solid 1px; color:#737373; }
.tb_l_basic caption { display:none; height:0; width:0; overflow:hidden; } 
