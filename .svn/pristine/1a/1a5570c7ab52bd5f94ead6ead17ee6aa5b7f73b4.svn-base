.tb_l_basic thead td { border-left:#e1e1e1 solid 1px; border-bottom:#e1e1e1 solid 1px; padding:3px 10px; }
.tb_l_basic thead td:first-child { border-left:none; }

.tb_l_basic tbody th { border-left:#e1e1e1 solid 1px; padding:8px 15px; border-bottom:#e1e1e1 solid 1px; background:#f7f7f7; font-weight:normal; text-align:center; color:#363636; }
.tb_l_basic tbody th:first-child { border-left:none; }
.tb_l_basic tbody td { border-left:#e1e1e1 solid 1px; border-bottom:#e1e1e1 solid 1px; padding:3px 15px; text-align:left; }
.tb_l_basic tbody td:first-child { border-left:none; }



.tb_input_basic { width:100%; border-top:#5271b0 solid 2px; border-bottom:#d4d4d4 solid 1px; margin-top:16px; color:#737373; }
.tb_input_basic caption { display:none; height:0; width:0; overflow:hidden; } 
