.tb_input_basic tbody th { border-left:#e1e1e1 solid 1px; border-right:#e1e1e1 solid 1px; padding:8px 15px 8px 15px; border-bottom:#e1e1e1 solid 1px; background:#f5f5f5; text-align:left; font-weight:normal; color:#363636; }
.tb_input_basic tbody th:first-child { border-left:none; }
.tb_input_basic tbody td { border-left:#e1e1e1 solid 1x; border-bottom:#e1e1e1 solid 1px; padding:8px 15px 8px 15px; }
.tb_input_basic tbody td:first-child { border-left:none; }



/* btn */
.tb_noti_set { float:none; *zoom:1; height:30px; line-height:30px; vertical-align:middle; margin-top:15px; overflow:hidden; }
.tb_noti_set:after { content:""; display:block; clear:both; height:0; }
.tb_noti_set .point { float:left; color:#f5543e; font-size:11px;  }
.tb_noti_set .btn { float:right; }

.tb_noti_btnzone { text-align:center; height:30px; line-height:30px; vertical-align:middle; margin-top:15px; overflow:hidden; }
.tb_noti_btnzone a { margin-left:10px; }
.tb_noti_btnzone a:first-child { margin-left:0px; }


/* input */
.input_b { width:170px; border:#cfcfcf solid 1px; line-height:1.8; height:20px; text-indent:4px; }
.textarea_b { width:95%; line-height:1.6; height:100px; overflow-y:scroll; border:#cfcfcf solid 1px; padding-left:4px;}



