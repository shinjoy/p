var now = new Date();

function openWbsActivityPop(type,activityId,startDate,endDate){
	popTargetActivityId = activityId;
	var url = "";
	var popTitle = "${baseUserLoginInfo.activityTitle}";
	var width = 1200;


	var params = {
					activityId:activityId,
					startDate:startDate,
					endDate:endDate,
					year:year
				 };

	if(type == "UPDATE"){
		url = "<c:url value='/project/updateActivityInfo.do'/>";
		popTitle+=" 수정";
		width = 800;

		var year = now.getFullYear();
		var endYear = endDate.split("-")[0];

		if(endYear !="9999"){
			if(endYear<year){
				year = endYear;
			}
		}
		 params = {
					activityId:activityId,
					startDate:startDate,
					endDate:endDate,
					year:year
				 };



	}else if(type == "HISTORY"){
		url = "<c:url value='/project/viewActivityUpdateHist.do'/>";
		popTitle+=" 수정이력";
		 params = {
					activityId:activityId
				 };
	}


	myModal2.open({
		windowID:"myModalCT",
		url: url,
		pars: params,
		titleBarText: popTitle,		//titleBarText 속성 추가하였음.(원 Axisj에는 없었던것)
		method:"POST",
		top: $(window).scrollTop() + 150,
		width: width,
		closeByEscKey: true			//esc 키로 닫기
	});

	$('#myModalCT').draggable();
}
