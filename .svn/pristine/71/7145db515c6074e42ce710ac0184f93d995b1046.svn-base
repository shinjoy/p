#ADM_container #IB_Snb .Snb_List li.current_single { background:#3054a1; }
#ADM_container #IB_Snb .Snb_List li.current_single a { background:url(../images/common/menu/bg_bu_admin_mn_current_single.gif) no-repeat 90% center; color:#fff; font-weight:bold; }
#ADM_container #IB_Snb .Snb_List li.current_single .Snb_de_List { display:block; }

#ADM_container #IB_Snb .Snb_List li.open_mn a { background:url(../images/common/menu/bg_bu_admin_mn_open.gif) no-repeat 90% center; font-weight:normal; }
#ADM_container #IB_Snb .Snb_List li.close_mn a { background:url(../images/common/menu/bg_bu_admin_mn_close.gif) no-repeat 90% center; font-weight:normal; }


#ADM_container #IB_Snb .Snb_List .Snb_de_List { background:#202631; border-top:#1d222c solid 1px; border-bottom:#1d222c solid 1px; padding:12px 0px 12px 12%; /*display:none;*/ } /*메뉴*/
#ADM_container #IB_Snb .Snb_List .Snb_de_List li { background:none; }
#ADM_container #IB_Snb .Snb_List .Snb_de_List li a { background:url(../images/common/menu/bg_bu_admin_mn_off.gif) no-repeat left center; color:#6177a5; line-height:18px; padding:6px 0px 6px 10px; display:block; font-weight:normal; border:none; }
#ADM_container #IB_Snb .Snb_List .Snb_de_List li a:hover { background:url(../images/common/menu/bg_bu_admin_mn_on.gif) no-repeat left center; color:#92ade8; font-weight:normal; }
#ADM_container #IB_Snb .Snb_List .Snb_de_List li a.current { background:url(../images/common/menu/bg_bu_admin_mn_on.gif) no-repeat left center; color:#92ade8; font-weight:normal; }



#contentsWrap { width:auto; overflow:hidden; background:url(../images/common/bg_snb.png) 0 0 repeat-y; box-sizing:border-box; position:relative; }
.contents { float:none; *zoom:1; background:url(../images/common/bg_htitlebar.png) repeat-x;  }
.contents:after { content:""; display:block; clear:both; height:0; }

.con_titleZone { float:none; *zoom:1; height:24px; background:url(../images/common/bg_htitlebar_start.png) no-repeat 0 0; padding:24px 30px 22px; }
.con_titleZone:after { content:""; display:block; clear:both; height:0; }
.con_titleZone .h2_title { float:left; color:#252525; font-size:20px; line-height:24px; height:24px; font-family:'NanumBarunL'; font-weight:bold; letter-spacing:-0.03em; }
.con_titleZone .location { float:right; background:url(../images/common/icon_location.gif) no-repeat left 9px; padding-left:20px; height:20px; padding-top:4px; color:#9ea3aa; }
.con_titleZone .divide { background:url(../images/common/bg_divide_loca.gif) no-repeat left center; padding:0 8px 0 10px; width:6px; height:20px; line-height:20px; vertical-align:middle; }
.con_titleZone .btn_PagePrint { margin-left:10px; }

#IB_footer { border-top:#dadada solid 1px; padding-top:20px; padding-bottom:30px; color:#a5a5a5; font-family:Verdana, Geneva, sans-serif; font-size:11px; text-align:center; }



/* 레프트 메뉴btn */
.btn_leftmn_op { position:absolute; left:-1px; top:170px;  z-index:2; display:none; }
.btn_leftmn_cl { position:absolute; left:177px; top:170px; z-index:2; }

/*마이퀵메뉴*/
.quickmnWrap { background:url(../images/common/bg_favor_mn.gif) repeat-x 0 bottom #e7e9e8; border-bottom:#b8b8bb solid 1px; }
.quickmnWrap:after { content:""; display:block; clear:both; height:0;}

/*숏코드*/
.shortcodeBox { float:left; width:141px; padding:6px 30px; background:url(../images/common/bg_div_quickmn.gif) repeat-y right 0; }
.shortcodeBox input { border:#afafaf solid 1px; height:22px; padding:2px 0px 2px 4px; width:100px; box-sizing:border-box; }
.shortcodeBox button { border-right:#afafaf solid 1px; border-top:#afafaf solid 1px; border-bottom:#afafaf solid 1px; height:22px; padding:0px 5px; line-height:1; font-size:11px; color:#666; background:url(../images/common/bg_btn_quickmn.gif) repeat-x #e6e8e7; width:40px; }

.myquickmenu { width:auto; overflow:hidden; }
.myquickmenu:after { content:""; display:block; clear:both; height:0;}
.quickmn_list { float:none; *zoom:1; }
.quickmn_list:after { content:""; display:block; clear:both; height:0 ;}
.quickmn_list li { line-height:1.4; float:left; margin-top:6px; }
.quickmn_list li a { float:left; padding:2px 8px; background:url(../images/common/bg_btn_quickmn.gif) repeat-x #e6e8e7; border:#bababa solid 1px; display:inline-block; margin-left:8px; border-radius:2px; }
.quickmn_list li:first-child a { margin-left:30px; }
.quickmn_list li:hover a { border:#b7b7b7 solid 1px; background:url(../images/common/bg_btn_quickmn_on.gif) repeat-x #dadada; color:#242424; }


.deatailconWrap { float:none; *zoom:1; background:url(../images/common/bg_detailcon.png) repeat-x 0 0; }
.deatailconWrap:after { content:""; display:block; clear:both; height:0; }

.deatailconWrap .addAreaZone { float:left; width:160px; padding:15px 20px 20px 15px; }
.deatailconWrap #detail_contents { border-left:#dadada solid 1px; }

#detail_contents { min-height:480px; padding:30px 30px 60px; box-sizing:border-box; width:auto; overflow:hidden; }
