/* splitter view */
#MySplitter { min-height: 745px; border-bottom:#c7cdd8 solid 1px; /* No padding allowed */}
#LeftPane {	border-left:#c7cdd8 solid 1px; border-right:#c7cdd8 solid 1px; border-top:#c7cdd8 solid 1px; /* No margin or border allowed */ }
#RightPane { border-bottom:#c7cdd8 solid 1px; border-left:#c7cdd8 solid 1px; background:url(../images/work/bg_right_block.gif) repeat-y right 0 #fff; /* No margin or border allowed */ }
