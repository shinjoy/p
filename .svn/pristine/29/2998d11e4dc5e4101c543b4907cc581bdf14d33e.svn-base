.mypageCardList li .front .count_new { background:url(../images/mypage/bg_count.png) no-repeat; width:33px; height:39px; text-align:center; color:#fff; position:absolute; left:50%; margin-left:38px; top:92px; line-height:34px; font-weight:bold; vertical-align:middle; }
.mypageCardList li .front .count_new span { vertical-align:middle; }
