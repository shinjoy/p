/*간트차트*/
.tb_ganttChart { width:100%; table-layout:fixed; }
.tb_ganttChart thead tr { border-left:#b9c1ce solid 1px; border-right:#b9c1ce solid 1px; }
.tb_ganttChart thead th { border-left:#c3cad5 solid 1px; border-right:none; border-top:#c3cad5 solid 1px; background:#f4f5f7; text-align:center; padding:3px 0; color:#8d8f93; font-size:11px; line-height:1.4; font-weight:normal; }
.tb_ganttChart thead td { border-left:none; background:url(../images/project/bg_th_left_gantt.gif) #f4f5f7 repeat-y left 0; text-align:center; padding:1px 0px; font-size:11px; line-height:1.4; color:#8d8f93; }
.tb_ganttChart thead tr:first-child th { border-top:none; }
.tb_ganttChart thead th:first-child { border-left:none; }
.tb_ganttChart thead td.divline { border-left:#c3cad5 solid 1px; }

.tb_ganttChart tbody tr { border-left:#b9c1ce solid 1px; border-right:#b9c1ce solid 1px; }
.tb_ganttChart tbody th { border-left:#c3cad5 solid 1px; border-top:none; border-bottom:none; background:#fff; text-align:center; padding:5px 0; color:#3a485e; font-weight:normal; }
.tb_ganttChart tbody td { border-left:#eee solid 1px; border-top:none; border-bottom:none; text-align:center; padding:0; color:#78818c; position:relative; }
.tb_ganttChart tbody td.divline { border-left:#c3cad5 solid 1px; }
.tb_ganttChart tbody td.start .ganttBar { padding-left:2px; }
.tb_ganttChart tbody td.start .startDate { left:2px; }
.tb_ganttChart tbody td.finish .ganttBar { padding-right:2px; }
.tb_ganttChart tbody td.finish .endDate { right:2px; }
.tb_ganttChart tbody td:first-child { background:none; }
.tb_ganttChart tbody th:first-child { border-left:none!important; }

.tb_ganttChart tbody .totalrow td { height:60px; }
.tb_ganttChart tbody tr:first-child .current .timeline { position:absolute; left:80%; top:0px; border-left:#ff4f4a dotted 1px; height:60px; z-index:4; }
.tb_ganttChart tbody tr:first-child .current .timeline .date { color:#ff4f4a; font-size:11px; position:absolute; padding-top:9px; margin-left:3px; }
.tb_ganttChart tbody .current .timeline { position:absolute; left:80%; top:1px; border-left:#ff4f4a dotted 1px; height:100%; z-index:4; }
.tb_ganttChart tbody .current .timeline .date { color:#ff4f4a; font-size:11px; position:absolute; padding-top:9px; margin-left:3px; }

.tb_ganttChart tr { border-left:none!important; border-right:none!important; }
.ganttBar { position:absolute; left:0px; bottom:10px; width:100%; float:none; *zoom:1; z-index:1; height:16px; box-sizing:border-box; position:absolute; }
.acti_RgList { width:100%; float:none; *zoom:1; margin-left:1px; }
.acti_RgList:after { content:""; display:block; clear:both; height:0; }

.acti_RgList li { width:100%; height:16px; display:inline-block; float:left; position:relative; }
.acti_RgList.n2 li { width:50%; }
.acti_RgList.n3 li { width:33.33%; }
.acti_RgList.n4 li { width:25%; }
.acti_RgList.n5 li { width:20%; }
.acti_RgList .no_acti_rg { background:#9f9f9f; border-left:#9f9f9f solid 1px; margin-left:-1px; }
.acti_RgList .etc1_rg { background:#32a7ec; border-left:#32a7ec solid 1px; margin-left:-1px; }
.acti_RgList .etc2_rg { background:#6db75e; border-left:#6db75e solid 1px; margin-left:-1px; }
.acti_RgList .etc3_rg { background:#CC33CC; border-left:#CC33CC solid 1px; margin-left:-1px; }
.acti_RgList .etc4_rg { background:#66FFFF; border-left:#66FFFF solid 1px; margin-left:-1px; }
.acti_RgList .etc5_rg { background:#ced6e0; border-left:#ced6e0 solid 1px; margin-left:-1px; }
.acti_RgList .done_rg { background:#f36968; border-left:#f36968 solid 1px; margin-left:-1px; }
.acti_RgList .acti_rg { background:#ced6e0; border-left:#ced6e0 solid 1px; margin-left:-1px; }
.acti_RgList .delay_rg { background:#ffa800; border-left:#ffa800 solid 1px; margin-left:-1px; }
.acti_RgList .incomplete_rg { background:#2e518b; border-left:#ffa800 solid 1px; margin-left:-1px; }
.acti_RgList li .num {color:#fff;font-weight:bold;font-size:9px;text-align:right;line-height:17px;vertical-align:middle;padding-right:3px;z-index:5;position: absolute; right:0;}

.startDate { position:absolute; left:0px; top:-23px; z-index:2; float:none; *zoom:1; width:100px; }
.startDate:after { content:""; display:block; clear:both; height:0; }
.startDate .txt_date { float:left; background:#ff4f4a; color:#fff; position:relative; display:inline-block; padding:2px 3px 3px; font-size:10px; line-height:1; letter-spacing:-0.04em; vertical-align:middle; }
.startDate .txt_date .point { background:#ff4f4a; position:absolute; left:0px; bottom:-5px; display:inline-block; z-index:3;  border-left:5px solid transparent; border-top:5px solid none; border-bottom:5px solid #fff; }
.startDate .txt_des { float:left; color:#ff4f4a; font-size:11px; margin-left:2px; vertical-align:middle; line-height:15px; }

.endDate { position:absolute; right:0px; top:-23px; z-index:2; float:none; *zoom:1; width:100px; }
.endDate:after { content:""; display:block; clear:both; height:0; }
.endDate .txt_date { float:right; background:#ff4f4a; color:#fff; position:relative; display:inline-block; padding:2px 3px 3px; font-size:10px; line-height:1; letter-spacing:-0.04em; vertical-align:middle; }
.endDate .txt_date .point { background:#ff4f4a; position:absolute; right:0px; bottom:-5px; display:inline-block; z-index:3;  border-right:5px solid transparent; border-top:5px solid none; border-bottom:5px solid #fff; }
.endDate .txt_des { float:right; color:#ff4f4a; font-size:11px; margin-right:2px; vertical-align:middle; line-height:15px; }

.multiLine2 { max-height:62px; overflow:hidden; display:-webkit-box; -webkit-line-clamp:2; -webkit-box-orient:vertical; word-wrap:break-word; text-overflow:ellipsis; }
.multiLine3 { max-height:62px; overflow:hidden; display:-webkit-box; -webkit-line-clamp:3; -webkit-box-orient:vertical; word-wrap:break-word; text-overflow:ellipsis; }


/* Tablet & Desktop Device */
@media all and (min-width:1440px){
/*사용자 해상도가 768px 이상일 때 이 코드가 실행됨. 테블릿과 데스크톱의 공통 코드를 작성한다.*/
.calandar_cubewrap li { width:50%; }
}

/* Tablet Device */
@media all and (min-width:1440px) and (max-width:1940px){
/*사용자 해상도가 768px 이상이고 1024px 이하일 때 이 코드가 실행됨. 아이패드 또는 비교적 작은 해상도의 랩탑이나 데스크톱에 대응하는 코드를 작성한다.*/
.calandar_cubewrap li { width:33.3%; }
}

/* Desktop Device */
@media all and (min-width:1940px){
/*사용자 해상도가 1025px 이상일 때 이 코드가 실행됨. 1025px 이상의 랩탑 또는 데스크톱에 대응하는 코드를 작성한다.*/
.calandar_cubewrap li { width:25%; }
}
