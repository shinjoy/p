/* 메모 목록 테이블  */
.memo_list_tb { letter-spacing:-0.08px; width:100%; border-bottom:#b9c1ce solid 1px; }
.memo_list_tb thead th { font-weight:bold; color:#3b4354; line-height:16px; padding:11px 0px 12px; text-align:center; border-bottom:#b9c1ce solid 1px; border-left:#b9c1ce solid 1px;  background:#d8dce3;  }
.memo_list_tb thead th:first-child { border-left:none; }

.memo_list_tb tbody th { font-weight:bold; }
