.calandar_cubewrap:after { content:""; display:block; clear:both; height:0;  }
.calandar_cubewrap li { float:left; width:50%; padding-left:20px; box-sizing:border-box; margin-bottom:20px; }

.calandar_cubewrap .calandar_cubeBox {  }
.calandar_cubeBox dt { text-align:center; padding:10px 0; background:#f1f1f1; border:#cfcfcf solid 1px; border-top-right-radius:5px; border-top-left-radius:5px; font-family:"NanumBarunL"; vertical-align:middle; border-bottom:#dadada solid 1px; }
.calandar_cubeBox .icon_cal_finish { background:url(../images/mypage/icon_calender_finish.png) no-repeat center center; width:12px; height:14px; display:inline-block; vertical-align:middle; opacity:0.6; }
.calandar_cubeBox .icon_cal_finish em { display:none; }
.calandar_cubeBox dt strong { font-size:17px; color:#101010; vertical-align:middle; }
.calandar_cubeBox dt span { font-size:13px; color:#9d9c9c; font-weight:600; vertical-align:middle; margin-left:8px; letter-spacing:0; }
.calandar_cubeBox dd { border-bottom-right-radius:5px; border-bottom-left-radius:5px; border:#cfcfcf 1px solid; border-top:none; height:230px; overflow-y:auto; overflow-z:hidden; }

.calandar_cube_tb { width:100%; table-layout:fixed; border-collapse:inherit; padding-bottom:10px; }
.calandar_cube_tb thead th { border-top:#dedede solid 1px; text-align:left; padding:7px 0px 7px 15px; background:#f6f6f6; color:#202020; border-right:#dedede solid 1px; }
.calandar_cube_tb thead td { border-top:#dedede solid 1px; padding:7px 15px 7px 15px; }
.calandar_cube_tb thead tr:first-child th, .calandar_cube_tb thead tr:first-child td { border-top:none; }

.calandar_cube_tb thead .lt_st01 { letter-spacing:0.1em; }
.calandar_cube_tb thead .lt_st02 { letter-spacing:-0.07em; }
.calandar_cube_tb thead td span { vertical-align:middle; }
.calandar_cube_tb thead td em { vertical-align:middle; font-size:11px; margin-left:4px; }
.calandar_cube_tb thead .lt_des01 { color:#797979; }
.calandar_cube_tb thead .lt_des02 { color:#eb2b2b; }

.calandar_cube_tb tbody td { padding:10px 15px 10px 15px; border-top:#cfcfcf dashed 1px; vertical-align:top; }
.calandar_cube_tb tbody .no_list { background:url(../images/mypage/bg_calendar_ing.gif) no-repeat center center; height:130px; }
.calandar_cube_tb tbody .no_list.finish { background:none; color:#a9a9a9; vertical-align:middle; text-align:center; }


.calandar_cube_tb .list_st_dot3 { margin-top:3px; }
.calandar_cube_tb .list_st_dot3 li {  float:none; *zoom:1; width:inherit; }
.calandar_cube_tb .list_st_dot3 li:after { content:""; display:block; clear:both; height:0;  }
.calandar_cube_tb .list_st_dot3 li span { vertical-align:middle; }
.calandar_cube_tb .list_st_dot3 li .issue_date { color:#555; }
.calandar_cube_tb .list_st_dot3 li .st_wrong { color:#ed4646; padding:0 3px; display:inline-block; background:#fce4dd; border:#eb947a solid 1px; margin:0 8px 0 12px; font-size:11px; }
.calandar_cube_tb .list_st_dot3 li .st_normal { color:#656565; padding:0 3px; display:inline-block; background:#f1f1f1; border:#b1b1b1 solid 1px; margin:0 8px 0 12px; font-size:11px; }
.calandar_cube_tb .list_st_dot3 li .issue_des { color:#999; text-align:left; }

/*마이페이지 메인*/
.mypageCardList { float:none; *zoom:1; margin-left:-20px; }
.mypageCardList:after { content:""; display:block; clear:both; height:0; }
.mypageCardList li { width:255px; height:326px; float:left; margin-left:20px; margin-bottom:20px; display:inline-block; }
.mypageCardList li .front { border:#cfcfcf solid 1px; background:#fff; border-radius:8px; position:relative;  box-shadow:0.5px 0.5px 3px rgba(80,80,80, 0.5); box-sizing:border-box; }
.mypageCardList li .front .card_title { font-family:"NanumBarunL"; font-size:20px; font-weight:bold; color:#202020; text-align:center; line-height:1; margin-top:50px; margin-bottom:15px; }
.mypageCardList li .front .card_con { color:#202020; }
.mypageCardList li .front .img_visual_icon { width:100px; height:100px; background:url(../images/mypage/bg_card_img02.gif) no-repeat center center; margin:0 auto; margin-bottom:38px; }
.mypageCardList li .front .cardmn_des { text-align:center; padding:0 30px; word-break:keep-all; color:#797979; line-height:1.4; }
.mypageCardList li .front .card_btnArea { display:none; }
.mypageCardList li .front .update_icon { background:url(../images/mypage/bg_new.png) no-repeat 0 0; width:74px; height:74px; border-top-left-radius:8px; display:none; position:absolute; left:-1px; top:-1px; }
