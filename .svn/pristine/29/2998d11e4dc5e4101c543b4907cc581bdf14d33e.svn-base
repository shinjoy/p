.state_icon_order { background:url(../images/work/icon_order.png) no-repeat 0 center; padding-left:15px; font-size:11px; font-weight:bold; font-family:"돋움"; color:#252525; display:inline-block; min-width:12px; height:12px; }
.state_icon_send { background:url(../images/work/icon_send.png) no-repeat 0 center; padding-left:15px; font-size:11px; font-weight:bold; font-family:"돋움"; color:#252525; display:inline-block; min-width:12px; height:12px; }
.state_icon_memo { background:url(../images/work/icon_memo.png) no-repeat 0 center; padding-left:15px; font-size:11px; font-weight:bold; font-family:"돋움"; color:#252525; display:inline-block; min-width:12px; height:12px; }
.state_icon_no_check { background:url(../images/work/icon_check_off.png) no-repeat 0 center; padding-left:15px; font-size:11px; font-weight:bold; font-family:"돋움"; color:#898989; display:inline-block; min-width:12px; height:12px; }

