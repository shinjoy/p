input.b_chst[type=checkbox]:disabled + label:before { content: ""; font-size:12px; font-weight:600; color:#8892a4; border:#8892a4 solid 1px; background:#eee; text-align: center; opacity:0.5; box-shadow:inset 0px 1px 1px 0px rgba(0, 0, 0, .2), 0px 1px 0px 0px rgba(255, 255, 255, .2); }
