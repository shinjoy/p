.mypageCardList li .back .card_con { color:#202020; }
.mypageCardList li .back .img_visual_icon { width:100px; height:100px; background:url(../images/mypage/bg_card_img02_back.gif) no-repeat center center; margin:0 auto; margin-bottom:10px; }
.mypageCardList li .back .cardmn_des { text-align:center; padding:0 30px; word-break:keep-all; color:#a5c1e1; line-height:1.4; }
.mypageCardList li .back .card_btnArea { display:block; text-align:center; margin-top:25px; }
.mypageCardList li .back .card_btnArea .rdmore_btn { border:#82b9e0 solid 1px; border-radius:20px; width:120px; height:25px; display:inline-block; text-align:center; color:#fff; font-family:"NanumBarunL"; font-size:12px; }
.mypageCardList li .back .update_icon { display:none; }
.mypageCardList li .back .update_icon em { display:none; }
.mypageCardList li .back .count_new { display:none; }

.mypageCardList li.update .front { /*box-shadow:2px 2px 5px rgba(100,100,100, 0.5);*/ }
.mypageCardList li.update .attendance_checkBox {/* box-shadow:2px 2px 5px rgba(100,100,100, 0.5);*/ }
.mypageCardList li.update .front .update_icon { display:block; }

.attendance_checkBox { border:#0573c1 solid 1px; background:#fff; border-radius:8px; position:relative;  box-shadow:1px 1px 3px rgba(180,180,180, 0.5); box-sizing:border-box; height:326px; }
.attendance_checkBox .card_title { font-family:"NanumBarunL"; font-size:20px; font-weight:bold; color:#202020; text-align:center; line-height:1; margin-top:50px; margin-bottom:35px; }
.mypageCardList li.update.attendance_checkBox .card_title { font-size:22px; color:#1a77b9; }
.attendance_checkBox .card_con { color:#202020; height:160px; }
.attendance_checkBox .card_btnArea { display:block; text-align:center; }
.attendance_checkBox .card_btnArea button { height:30px; display:inline-block; margin-left:10px; background:#b0b0b0; width:92px; text-align:center; border-radius:3px; line-height:1; color:#fff; font-family:"돋움",Dotum,"굴림",Gulim,arial,sans-serif; font-size:12px; font-weight:bold; }
.attendance_checkBox .card_btnArea button:first-child { margin-left:0px; }
.attendance_checkBox .card_btnArea button.need_check { background:#3054a1; }

.attendance_checkBox .update_icon { background:url(../images/mypage/bg_new.png) no-repeat 0 0; width:74px; height:74px; border-top-left-radius:8px; display:block; position:absolute; left:-1px; top:-1px; }
.attendance_checkBox .update_icon em { display:none; }
.attendance_checkBox .date_box { font-family:"NanumBarunL"; font-weight:bold; font-size:13px; color:#202020; text-align:center; line-height:1; }
.attendance_checkBox .date_box span { margin-right:4px;}
.attendance_checkBox .date_box em { font-weight:bold; }
.attendance_checkBox .time_box { background:url(../images/mypage/bg_timebox.gif) no-repeat 24px 18px #f5f5f5; border:#cfcfcf solid 1px; border-radius:58px; height:58px; line-height:58px; padding:0 20px 0 51px; font-size:25px; font-family:"NanumBarun"; font-weight:bold; width:193px; box-sizing:border-box; margin:10px auto 5px; letter-spacing:0.04em; }
.attendance_checkBox .ip_box { text-align:center; vertical-align:middle; }
.attendance_checkBox .ip_box span { vertical-align:middle; color:#929292; font-family:Tahoma, Geneva, sans-serif; font-size:11px; }
.attendance_checkBox .icon_PC { width:15px; height:10px; display:inline-block; background:url(../images/mypage/icon_pc.gif) no-repeat center center; margin-right:5px; }
.attendance_checkBox .icon_secom { width:15px; height:10px; display:inline-block; background:url(../images/mypage/icon_secom.gif) no-repeat center center; margin-right:5px; }
.attendance_checkBox .icon_mobile { width:7px; height:11px; display:inline-block; background:url(../images/mypage/icon_mobile.gif) no-repeat center center; margin-right:5px; }
.attendance_checkBox .icon_PC em, .attendance_checkBox .icon_secom em, .attendance_checkBox .icon_mobile em { display:none; }
.attendance_checkBox .atten_result { color:#888; text-align:center; margin-top:15px; font-size:11px; }
.attendance_checkBox .atten_result.offwork { margin-top:0px; }
.attendance_checkBox .atten_result .wrong { color:#eb2b2b; }
.attendance_checkBox .atten_result .good { color:#3b7fca; }

