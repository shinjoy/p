/* 대화창 */
.chat_flow { overflow-x:hidden; overflow-y:auto; height:500px; padding:5px 15px 5px 15px; /* border-right:#c7cdd8 solid 1px; */ background:#f5f6f8; }
.communiBox { width:100%; float:none; *zoom:1; margin-top:20px; }
.communiBox:first-child { margin-top:0px; }
.communiBox:last-child { margin-bottom:20px; }
.communiBox:after { content:""; display:block; clear:both; height:0; }
.communiBox .pic { background:url(../images/work/bg_pic.png) no-repeat; width:52px; height:40px; display:inline-block; }
.communiBox .pic img { width:40px; height:40px; border-radius:40px; }
.communiBox .conBox { margin-left:52px; margin-top:-47px; padding-right:20%; }
.communiBox .conBox li { float:left; clear:both; margin-bottom:10px; }
.communiBox .nametime { vertical-align:middle; margin-top:5px; padding-left:3px; margin-bottom:3px!important; }
.communiBox .nametime strong { font-weight:bold; color:#262b33; font-family:"돋움"; letter-spacing:-0.5px; }
.communiBox .nametime span { font-weight:normal; color:#979fac; font-size:11px; vertical-align:middle; margin-left:10px; }
.communiBox .nametime span:first-child { margin-left:0px; }
.communiBox .nametime strong + .communiBox .nametime span { margin-left:10px; }

.communiBox .nametime .count { color:#e95c1e; font-weight:bold; }
.communiBox .bubble { float:left; background:#fff; border:#cacdd0 solid 1px; border-radius:3px; padding:6px 8px 7px; line-height:16px; position:relative; box-shadow:#e7eaee 1px 1px 1px; text-align:left; word-break:break-all; }
.communiBox .bubble .arrow { position:absolute; left:-10px; top:7px; background:url(../images/work/bubble_arrow.png) no-repeat; width:10px; height:9px; }

/* 대화창 오른쪽 */
.communiBoxRignt { width:100%; float:none; *zoom:1; margin-top:0px; }
.communiBoxRignt:first-child { margin-top:0px; }
