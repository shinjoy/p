.button, .button a, .button button, .button input {
	position : relative;
	margin : 0;
	display : inline-block;
	border : 0;
	white-space : nowrap;
	vertical-align : middle;
	overflow : visible;
	font-size:9pt; color: #ffffff;
}
.button {
	height : 20px;
	padding : 0;
	margin-right : 2px;
	background-position : left top;
}
.button a, .button button, .button input {
	left : 2px;
	cursor : pointer;
}
.button a, .button button, .button input {
	height : 20px;
	padding : 0 8px 0 8px;
	line-height : 20px;
	background-position : right top;
}
.button *:hover, .button *:active, .button *:focus {
	color : red;
}
.button a {
	padding : 0 10px 0 10px;
}
.btn_Blue, .btn_Blue a, .btn_Blue button, .btn_Blue input {
	background-image:url(/SynergyCus/images/sp/CSS/button_blue.gif);
}
.btn_Orange, .btn_Orange a, .btn_Orange button, .btn_Orange input {
	background-image: url(/SynergyCus/images/sp/CSS/button_orange.gif);
}
@page a4sheet {
	size:21.0cm 29.7cm;
}
.a4 {
	page:a4sheet;
	page-break-after:always;
}
.page_size {
	width:21.0cm;
	height:29.7cm;
}
.text {
	font-weight:bold;
	font-size:15pt;
}
a:link {
	font-size:9pt;
	text-decoration:none;
	color:darkblue;
}
* {
	font-size:9pt;
}
/* 메인페이지 상단 */
.page_top {
	width:100%;
	text-align:center;
	border-top:5px solid #0066FF;
	border-left:5px solid #0066FF;
	border-right:5px solid #0066FF;
}
/* 메인페이지 하단 */
.page_bottom {
	width:100%;
	text-align:center;
	border-left:5px solid #CC9900;
	border-right:5px solid #CC9900;
	border-bottom:5px solid #CC9900;
}
/* 컨텐츠페이지 */
.content {
	margin-left:2%;
	width:96%;
}
.confirm {
	font-weight:bold;
	font-size:15pt;
	text-align:right;
	line-height:150%;
	padding-right:40px;
}

a:link {
	font-size:9pt;
	text-decoration:none;
	color:darkblue;
}

a:visited {
	text-decoration:none;
	color:darkblue;
}

a:hover {
	/*text-decoration:underline;*/
	color:red;
}

a:active {
	text-decoration:none;
	color:red;
}